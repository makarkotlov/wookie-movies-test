{
  "name": "wookiemovies",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "scripts/postinstall.sh",
    "android:dev": "react-native run-android --variant=debug --appIdSuffix 'debug'",
    "android:staging": "react-native run-android --variant=releaseStaging --appIdSuffix 'staging'",
    "android:prod": "react-native run-android --variant=releaseDebug",
    "ios:dev": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix index.* src",
    "reset": "scripts/reset.sh",
    "icons": "fantasticon & npx react-native link"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^7.1.3",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/elements": "^1.2.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "axios": "^0.24.0",
    "humps": "^2.0.1",
    "mobx": "^6.3.8",
    "mobx-react": "^7.2.1",
    "promise.allsettled": "^1.0.5",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-image-header-scroll-view": "^1.0.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-star-view": "^1.1.3"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/humps": "^2.0.1",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-native": "^0.66.6",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^27.4.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "date-fns": "^2.27.0",
    "eslint": "7.14.0",
    "fantasticon": "^1.2.3",
    "jest": "^27.4.3",
    "jest-circus": "^27.4.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest.setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  }
}
