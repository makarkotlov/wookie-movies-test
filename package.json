{
  "name": "wookiemovies",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "scripts/postinstall.sh",
    "android:dev": "react-native run-android --variant=debug --appIdSuffix 'debug'",
    "android:staging": "react-native run-android --variant=releaseStaging --appIdSuffix 'staging'",
    "android:prod": "react-native run-android --variant=releaseDebug",
    "ios:dev": "react-native run-ios --scheme Debug --simulator \"iPhone 5s\"",
    "ios:staging": "react-native run-ios --scheme Staging --simulator \"iPhone 5s\"",
    "ios:prod": "react-native run-ios --scheme Release --simulator \"iPhone 5s\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix index.* src",
    "reset": "scripts/reset.sh"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-config": "^1.4.5"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^8.0.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-native": "^0.66.6",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^27.4.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^27.4.3",
    "jest-circus": "^27.4.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$",
    "collectCoverage": false,
    "coverageReporters": [
      "html"
    ]
  }
}
